<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vincent.hhxz">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AutoImageActivity" />
        <activity android:name=".activity.GoodsCategoryActivity" />
        <activity android:name=".activity.SpecialActivity" />
        <activity android:name=".activity.SpecialListActivity" />
        <activity android:name=".activity.GoodsShareActivity" />
        <activity android:name=".activity.GoodsRecommandActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.HotSearchGoodsActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SettingActivity"></activity>
    </application>

</manifest>